{
    "name": "partberlin/advlaynav",
    "description": "Advanced Layered Navigation for Magento2",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "magento/module-store": ">=100.0.0",
        "magento/module-backend": ">=100.0.0",
        "magento/module-catalog": ">=100.0.0",
        "magento/module-catalog-search": ">=100.0.0",
        "magento/module-eav": ">=100.0.0",
        "magento/module-layered-navigation": ">=100.0.0",
        "magento/framework": ">=100.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.5",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "^1.0",
        "phpdocumentor/phpdocumentor": "^2.8"
    },
    "type": "magento2-module",
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Part\\AdvLayNav\\": ""
        }
    }
}
